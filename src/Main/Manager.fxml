<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" style="-fx-background-color: #ffff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.ManagerController">
   <children>
      <BorderPane prefHeight="800.0" prefWidth="1000.0">
         <top>
            <Pane onMouseDragged="#mouseDraged" onMousePressed="#mousePressed" prefHeight="37.0" prefWidth="1000.0" style="-fx-background-color: F2F2F2;" BorderPane.alignment="CENTER">
               <children>
                  <Button layoutX="972.0" layoutY="2.0" mnemonicParsing="false" onAction="#exit" prefHeight="10.0" prefWidth="10.0" style="-fx-background-color: F2F2F2;">
                     <graphic>
                        <ImageView fitHeight="14.0" fitWidth="12.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/exit.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button layoutX="951.0" layoutY="2.0" mnemonicParsing="false" onAction="#minimize" prefHeight="25.0" prefWidth="15.0" style="-fx-background-color: F2F2F2;">
                     <graphic>
                        <ImageView fitHeight="13.0" fitWidth="11.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/minimize.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button contentDisplay="CENTER" graphicTextGap="1.0" layoutY="-1.0" mnemonicParsing="false" onAction="#extendedNavigationBar" prefHeight="20.0" prefWidth="20.0" style="-fx-background-color: F2F2F2;">
                     <graphic>
                        <ImageView fitHeight="33.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true" smooth="false">
                           <image>
                              <Image url="@../images/navigationButton.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
               <effect>
                  <DropShadow color="#0000004e" />
               </effect>
            </Pane>
         </top>
         <left>
            <Pane fx:id="navigationBar" prefHeight="768.0" prefWidth="200.0" style="-fx-background-color: FED755;" translateX="-210.0" BorderPane.alignment="CENTER">
               <effect>
                  <DropShadow blurType="ONE_PASS_BOX" color="#00000046" height="0.0" offsetY="3.0" radius="9.94" width="41.76" />
               </effect>
               <children>
                  <VBox layoutX="15.0" layoutY="18.0" prefHeight="491.0" prefWidth="173.0" style="-fx-spacing: 30;">
                     <children>
                        <Button alignment="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="169.0" style="-fx-background-color: white;" text="Manager" textFill="#fed755" />
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="171.0" style="-fx-background-color: white;" text="Button" textFill="#fed755" />
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="172.0" style="-fx-background-color: white;" text="Button" textFill="#fed755" />
                     </children>
                  </VBox>
               </children>
            </Pane>
         </left>
         <center>
            <Pane prefHeight="724.0" prefWidth="542.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox layoutX="9.0" layoutY="31.0" prefHeight="683.0" prefWidth="578.0" style="-fx-spacing: 30;">
                     <children>
                        <HBox prefHeight="35.0" prefWidth="292.0" style="-fx-spacing: 10;">
                           <children>
                              <ImageView fitHeight="29.0" fitWidth="45.0" opacity="0.5" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/search.png" />
                                 </image>
                              </ImageView>
                              <TextField prefHeight="25.0" prefWidth="250.0" promptText="Search" style="-fx-background-color: white; -fx-background-radius: 10;">
                                 <effect>
                                    <DropShadow color="#0000001f" />
                                 </effect>
                              </TextField>
                           </children>
                        </HBox>
                        <HBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="28.0" prefWidth="322.0" spacing="10.0">
                           <children>
                              <Button mnemonicParsing="false" style="-fx-background-color: white; -fx-background-radius: 10;" text="Capacity">
                                 <effect>
                                    <DropShadow color="#69696924" />
                                 </effect>
                              </Button>
                              <Button mnemonicParsing="false" style="-fx-background-color: white; -fx-background-radius: 10;" text="Available">
                                 <effect>
                                    <DropShadow color="#69696924" />
                                 </effect>
                              </Button>
                              <Button mnemonicParsing="false" style="-fx-background-color: white; -fx-background-radius: 10;" text="Hot">
                                 <effect>
                                    <DropShadow color="#69696924" />
                                 </effect>
                              </Button>
                              <Button mnemonicParsing="false" style="-fx-background-color: white; -fx-background-radius: 10;" text="Sale">
                                 <effect>
                                    <DropShadow color="#69696924" />
                                 </effect>
                              </Button>
                              <Button mnemonicParsing="false" style="-fx-background-color: white; -fx-background-radius: 10;" text="Service">
                                 <effect>
                                    <DropShadow color="#69696924" />
                                 </effect>
                              </Button>
                           </children>
                        </HBox>
                        <ScrollPane fx:id="listOfPanels" hbarPolicy="NEVER" prefHeight="546.0" prefWidth="578.0" style="-fx-background-color: white;">
                           <content>
                              <VBox fx:id="vstackList" alignment="CENTER" prefHeight="540.0" prefWidth="570.0" style="-fx-background-color: white; -fx-spacing: 5;" />
                           </content>
                        </ScrollPane>
                     </children>
                  </VBox>
               </children>
            </Pane>
         </center>
         <right>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </children>
</AnchorPane>
